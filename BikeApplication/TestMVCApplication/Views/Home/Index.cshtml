@{
    ViewData["Title"] = "Home Page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/twins_pre_event.jpg" alt="ASP.NET" class="img-responsive" />
        </div>

        <div class="row">
            <div class="col-md-3">
                <h2>Light weight</h2>
            </div>
            <div class="col-md-3">
                <h2>Tunable</h2>
            </div>
            <div class="col-md-3">
                <h2>Stripped Down</h2>
            </div>
        </div>


        <p>
            Popular among British Motorcyclists in the early 1960's, Royal Enfield brings back a modern translation of the Continental GT from 1965 in 2014. The café racer is a motorcycle, purpose-built for speed and handling.
        </p>
